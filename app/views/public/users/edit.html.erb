<h1>編集</h1>
<%= form_with model: @user, method: :patch do |f| %>
  <div>
    <%= f.label :profile_image, "プロフィール画像" %><br>
    <%= f.file_field :profile_image, accept: "image/*" %>
  </div>
  <div>
    <%= f.label :name, "アカウント名" %><br>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :introduction, "自己紹介" %><br>
    <%= f.text_area :introduction %>
  </div>
  <%= f.submit "更新" %>
<% end %>
